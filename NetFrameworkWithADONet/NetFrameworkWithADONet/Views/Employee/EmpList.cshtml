@model List<NetFrameworkWithADONet.Context.EMPTable>
@{
    ViewBag.Title = "EmpList";
}

<table class="table table-hover">
    <tr>
        <th>EmpID</th>
        <th>EmpName</th>
        <th>EmpLastname</th>
        <th>EmailId</th>
      
        <th>City</th>
        <th>Contact No</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.EmpID</td>
            <td>@item.EmpName</td>
            <td>@item.EmpLastname</td>
            <td>@item.EmailId</td>
           
            <td>@item.City</td>
            <td>@item.ContactNo</td>
            <td>
                <a href="@Url.Action("EmpView",new { item.EmpID,item.EmpName,item.EmpLastname,item.EmailId,item.Password,item.City,item.ContactNo })"><i class="glyphicon glyphicon-pencil"></i></a>
                <a href="@Url.Action("Delete",new { ID=@item.EmpID})"><i class="glyphicon glyphicon-trash"></i></a>
            </td>
        </tr>
    }


</table>

